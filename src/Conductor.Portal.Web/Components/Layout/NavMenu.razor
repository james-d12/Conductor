<MudNavMenu Class="paypoint-navmenu">
    <MudTooltip Text="Home" Placement="Placement.Right">
        <MudNavLink Href="/" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home" IconColor="Color.Inherit"/>
    </MudTooltip>

    <MudTooltip Class="navlink" Text="Repositories" Placement="Placement.Right">
        <MudNavLink Href="repositories" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Build"
                    IconColor="Color.Inherit"/>
    </MudTooltip>

    <MudTooltip Text="Projects" Placement="Placement.Right">
        <MudNavLink Href="projects" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Apps"
                    IconColor="Color.Inherit"/>
    </MudTooltip>

    <MudTooltip Text="Packages" Placement="Placement.Right">
        <MudNavLink Href="packages" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Extension"
                    IconColor="Color.Inherit"/>
    </MudTooltip>
</MudNavMenu>